    "\n",
    "\n",
    "# Optional: Plot the history of the points\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "history = np.array(history)\n",
    "plt.plot(history[:, 0], history[:, 1], 'o-', markersize=3, label='Path of gradient descent')\n",
    "plt.xlabel('x')\n",
    "plt.ylabel('y')\n",
    "plt.title('Gradient Descent Path')\n",
